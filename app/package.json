{
  "name": "DoubanFM",
  "version": "1.0.0",
  "description": "DoubanFM desktop version built on electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-win": "electron-packager . doubanFM --platform=win32 --arch=x64 --version=1.2.6 --icon=./fm.ico --overwrite",
    "build-mac": "electron-packager . doubanFM --ignore=node_modules/electron-* --platform=darwin --arch=x64 --version=1.2.6 --icon=./fm.icns --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boxizen/DoubanFM.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/boxizen/DoubanFM/issues"
  },
  "homepage": "https://github.com/boxizen/DoubanFM#readme",
  "devDependencies": {
    "electron-builder": "^5.12.1",
    "electron-localshortcut": "^0.6.1",
    "electron-prebuilt": "^1.2.0"
  },
  "dependencies": {
    "ipc": "0.0.1"
  }
}
